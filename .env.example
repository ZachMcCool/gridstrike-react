# GridStrike React App Environment Variables
# Copy this file to .env and fill in your actual values

# Firebase Configuration - Get these from your Firebase project
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# OpenAI Configuration - Get your API key from https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Application Settings
VITE_DETAILED_ERRORS=true
VITE_LOG_LEVEL=Information

# Firestore Collections
VITE_COLLECTION_CARDS=cards
VITE_COLLECTION_DECKS=decks
